<template>
  <canvas canvas-id="{{id}}" />
</template>
<script>
  import wepy from 'wepy'

  export default class CV extends wepy.component {
    data ={
    }
    props = {
      id: {
        type: [String, Number],
        twoWay: false
      }
    }
    onLoad() {
      const ctx = wx.createCanvasContext(this.id)
      ctx.setFillStyle('red')
      ctx.fillRect(10, 10, 150, 75)
      ctx.draw()
    }
  }
</script>
